[run]
branch = True
omit =
    web_search_sdk/browser.py
    web_search_sdk/scrapers/*_legacy.py
    web_search_sdk/scrapers/news.py
    web_search_sdk/scrapers/related.py
    web_search_sdk/scrapers/stock.py
    web_search_sdk/scrapers/wikipedia.py
    web_search_sdk/utils/http_logging.py
    web_search_sdk/utils/logging.py
    web_search_sdk/utils/rate_limit.py
    web_search_sdk/utils/requests_logging.py
    web_search_sdk/utils/dates.py
    web_search_sdk/utils/text.py
    web_search_sdk/scrapers/trends.py
    web_search_sdk/utils/http.py
    web_search_sdk/scrapers/paywall.py

[report]
fail_under = 80
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.: 